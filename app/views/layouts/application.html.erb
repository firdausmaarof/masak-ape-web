<script type="text/javascript">
    jQuery(document).ready(function($) {
        $(".scroll").click(function(event){     
            event.preventDefault();
            $('html,body').animate({scrollTop:$(this.hash).offset().top},1200);
        });
    });
</script>
</head>
<body>
    <!-- header-section-starts -->
    <div class="header">
        <div class="container">
            <div class="top-header">
                <div class="logo">
                    <a href="index.html"><img src="<%= image_path('logo.png') %>" class="img-responsive" alt="" /></a>
                </div>
                
                <div class="header-right">
                    <div class="queries">
                        <p>Questions? Call us!<span>013-5031094 </span><label>(11AM to 11PM)</label></p>
                    </div>
                    <!-- <div class="cart box_1">
                        <a href="checkout.html">
                            <h3> <span class="simpleCart_total"> $0.00 </span> (<span id="simpleCart_quantity" class="simpleCart_quantity"> 0 </span> items)<img src="/assets/bag.png" alt=""></h3>
                        </a>    
                        <p><a href="javascript:;" class="simpleCart_empty">Empty card</a></p>
                        <div class="clearfix"> </div>
                    </div> -->
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
            <div class="menu-bar">
            <div class="container">
                <div class="top-menu">
                    <ul>
                        <li>
                            <%= link_to "Home", root_path, 'data-no-turbolink' => true %>
                        </li>
                        <% if current_user %>
                        |<li>
                            <%= link_to "Ingredient", ingredients_path %>
                        </li>|
                        <li>
                            <%= link_to "Cuisine", cuisines_path %>
                        </li>|
                        <li>
                            <%= link_to "Occasion", occasions_path %>
                        </li>|
                        <li>
                            <%= link_to "Recipe", recipes_path %>
                        </li>
                        <% end %>
                        <div class="clearfix"></div>
                    </ul>
                </div>
                <div class="login-section">
                    <ul>
                        <% if current_user %>
                            <li>
                                <%= link_to "Logout", destroy_user_session_path, method: :delete %>
                            </li>
                        <% else %>
                            <li>
                                <%= link_to "Login", new_user_session_path  %>
                            </li> |
                            <li>
                                <%= link_to "Register", new_user_registration_path %>
                            </li>
                        <% end %>
                        <div class="clearfix"></div>
                    </ul>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    <!-- header-section-ends -->
    <!-- Page Content -->
    <% if false %>
    <% flash.each do |a, b| %>
        <div class="alert alert-success"><%= b %></div>
    <% end %>
    <% end %>
    <%= yield %>

    <!-- /.container -->
    <!-- footer-section-starts -->
    <div class="footer">
        <div class="container">
            <p class="wow fadeInLeft" data-wow-delay="0.4s">&copy; 2014  All rights  Reserved | Template by &nbsp;<a href="http://w3layouts.com" target="target_blank">W3Layouts</a></p>
        </div>
    </div>

    
 

    <!-- footer-section-ends -->
      <script type="text/javascript">
        $(document).ready(function() {
